<% title "Контакты" %>

<span style="float: right;"><%= link_to "Новый контакт", new_admin_contact_path %></span>

<% for contact in @contacts %>

  <%= contact.contact %>
  <div style="float: right; margin-bottom: 20px;">
    <%= link_to "Редактировать", edit_admin_contact_path(contact) %>
    <%= link_to "Удалить", admin_contact_path(contact), :confirm => 'Вы уверены?', :method => :delete %>
  </div>
<% end %>


